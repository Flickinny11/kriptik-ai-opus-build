{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/orchestrate.ts": {
      "maxDuration": 900,
      "memory": 3008
    },
    "api/modal-webhook.ts": {
      "maxDuration": 10,
      "memory": 512
    }
  },
  "rewrites": [
    { "source": "/api/modal-webhook", "destination": "/api/modal-webhook" },
    { "source": "/api/orchestrate", "destination": "/api/orchestrate" },
    { "source": "/(.*)", "destination": "/api" }
  ]
}
