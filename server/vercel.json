{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**/*.ts", "src/**/*.js", "src/**/*.py", "package.json", "tsconfig.json"]
      }
    },
    {
      "src": "api/orchestrate.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**/*.ts", "src/**/*.js", "src/**/*.py", "package.json", "tsconfig.json"]
      }
    }
  ],
  "functions": {
    "api/orchestrate.ts": {
      "maxDuration": 900,
      "memory": 3008
    }
  },
  "routes": [
    { "src": "/api/orchestrate", "dest": "/api/orchestrate.ts" },
    { "src": "/(.*)", "dest": "/api/index.ts" }
  ]
}
