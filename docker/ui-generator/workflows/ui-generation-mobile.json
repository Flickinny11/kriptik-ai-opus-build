{
  "1": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "flux1-dev.safetensors"
    },
    "_meta": {
      "title": "Load FLUX.1-dev Checkpoint"
    }
  },
  "2": {
    "class_type": "LoraLoader",
    "inputs": {
      "lora_name": "ui-design/ui-design-lora.safetensors",
      "strength_model": 0.85,
      "strength_clip": 0.85,
      "model": ["1", 0],
      "clip": ["1", 1]
    },
    "_meta": {
      "title": "Load UI-Design LoRA"
    }
  },
  "3": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "kriptik_ui, professional mobile app UI design, iPhone screen, modern clean interface, high fidelity mockup",
      "clip": ["2", 1]
    },
    "_meta": {
      "title": "Positive Prompt (Mobile)"
    }
  },
  "4": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "blurry, low quality, distorted text, broken layout, watermark, signature, amateur, ugly, deformed, desktop",
      "clip": ["2", 1]
    },
    "_meta": {
      "title": "Negative Prompt"
    }
  },
  "5": {
    "class_type": "EmptyLatentImage",
    "inputs": {
      "width": 430,
      "height": 932,
      "batch_size": 1
    },
    "_meta": {
      "title": "Empty Latent (Mobile 430x932 - iPhone 14 Pro)"
    }
  },
  "6": {
    "class_type": "KSampler",
    "inputs": {
      "seed": 0,
      "steps": 8,
      "cfg": 3.5,
      "sampler_name": "euler",
      "scheduler": "normal",
      "denoise": 1.0,
      "model": ["2", 0],
      "positive": ["3", 0],
      "negative": ["4", 0],
      "latent_image": ["5", 0]
    },
    "_meta": {
      "title": "KSampler (8 Steps)"
    }
  },
  "7": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["6", 0],
      "vae": ["1", 2]
    },
    "_meta": {
      "title": "VAE Decode"
    }
  },
  "8": {
    "class_type": "SaveImage",
    "inputs": {
      "filename_prefix": "kriptik_ui_mobile",
      "images": ["7", 0]
    },
    "_meta": {
      "title": "Save Mobile UI Mockup"
    }
  }
}
