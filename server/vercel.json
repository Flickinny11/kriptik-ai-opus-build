{
  "version": 2,
  "functions": {
    "api/index.ts": {
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": "src/**"
    },
    "api/orchestrate.ts": {
      "maxDuration": 900,
      "memory": 3008,
      "includeFiles": "src/**"
    },
    "api/modal-webhook.ts": {
      "maxDuration": 10,
      "memory": 512,
      "includeFiles": "src/**"
    }
  },
  "rewrites": [
    { "source": "/api/modal-webhook", "destination": "/api/modal-webhook" },
    { "source": "/api/orchestrate", "destination": "/api/orchestrate" },
    { "source": "/(.*)", "destination": "/api" }
  ]
}
